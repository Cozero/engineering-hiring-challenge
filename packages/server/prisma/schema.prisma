generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Metric {
  reported_at       DateTime? @db.Timestamptz(6)
  organization_name String?
  emisions          Float?
  id                Int       @id @default(autoincrement())
  @@map(name: "metrics")
}
